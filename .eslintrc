{
  parserOptions: {
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true,
        "module": true,
        "modules": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "globals": {},
  "rules": {
    "strict": 0,
    "eol-last": 0,
    "space-before-function-paren": [
        2,
        {"anonymous": "always", "named": "never"}
    ],
    "no-shadow": 2,
    "max-statements": [2, 20],
    "one-var": 2,
    "semi": 2,
    "semi-spacing": 2,
    "brace-style": [2, "stroustrup", { "allowSingleLine": true }],
    "space-before-blocks": [2, "always"],
    "space-in-parens": [2, "never"],
    "keyword-spacing": [2, {"before": true, "after": true, "overrides": {}}],
    "no-nested-ternary": 2,
    "no-multiple-empty-lines": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-mixed-requires": 0,
    "comma-style": [2, "last"],
    "comma-spacing": [2, { "before": false, "after": true }],
    "no-undef": 2,
    "no-unused-vars": 2,
    "quotes": [2, "single"],
    "no-use-before-define": [2, "nofunc"],
    "camelcase": 2,
    "eqeqeq": 0,
    "new-cap": 2,
    "no-underscore-dangle": 0,
    "no-extend-native": 0,
    "yoda": [2, "never"],
    "no-cond-assign": 2,
    "no-console": 2,
    "complexity": [2, 5],
    "curly": [2, "all"],
    "dot-notation": 2,
    "no-alert": 2,
    "no-else-return": 2,
    "no-labels": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new-wrappers": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-self-compare": 2,
    "no-unused-expressions": 2,
    "no-warning-comments": 1,
    "no-with": 2,
    "vars-on-top": 2,
    "wrap-iife": 2
  }
}