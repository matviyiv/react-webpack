{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "react app with hot reload",
  "main": "client/app.js",
  "private": true,
  "scripts": {
    "test": "karma start karma.config.js",
    "test-watch": "karma start karma.config.js --no-single-run --auto-watch",
    "hot": "webpack-dev-server --config webpack-hot-dev-server.config.js --hot --progress --colors --port 2992 --inline",
    "build": "webpack --config webpack-production.config.js --progress --profile --colors",
    "dev-server": "nodemon ./server/index.js",
    "start-dev": "npm run hot & npm run dev-server && fg",
    "start-prod": "NODE_ENV=prod node ./server/index.js",
    "start": "npm run build && npm run start-prod",
    "lint": "eslint -c .eslintrc --fix ./server/**/*.js"
  },
  "keywords": [
    "webpack",
    "react",
    "redux",
    "hot"
  ],
  "author": "Nazar Matviyiv",
  "license": "MIT",
  "dependencies": {
    "async": "^1.5.2",
    "babel-core": "^6.5.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.3",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "basscss-sass": "^1.0.2",
    "body-parser": "^1.14.2",
    "cookie-parser": "^1.4.0",
    "css-loader": "^0.14.0",
    "es6-promise": "^3.0.2",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.16.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.0",
    "fetch-polyfill": "^0.8.1",
    "file-loader": "^0.8.1",
    "history": "^1.17.0",
    "html-loader": "^0.3.0",
    "less": "^2.1.2",
    "less-loader": "^2.0.0",
    "lodash": "^3.9.3",
    "node-sass": "3.3.3",
    "null-loader": "^0.1.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-lazy-load": "^1.0.4",
    "react-proxy-loader": "^0.3.1",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.3",
    "react-transform-hmr": "^1.0.1",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.2",
    "sass-loader": "0.5.0",
    "style-loader": "^0.12.0",
    "superagent": "^1.1.0",
    "url-loader": "^0.5.5",
    "webpack": "^1.8.5",
    "webpack-dev-server": "^1.4.7"
  },
  "devDependencies": {
    "bluebird": "^3.0.5",
    "expect": "^1.13.0",
    "ghooks": "^0.3.2",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-cli": "^0.1.1",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.2",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "phantomjs": "^1.9.19",
    "react-addons-test-utils": "^0.14.3",
    "redux-devtools": "^2.1.5",
    "sinon": "git+https://github.com/sinonjs/sinon.git"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run test"
    }
  }
}
